{
  "name": "@monorepo-utilities/install-dependencies",
  "version": "0.1.8",
  "description": "Builds node modules to a spectified path ðŸ“¦",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "bin": {
    "install-dependencies": "./dist/index.js",
    "install-deps": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts --config ../../.eslintrc",
    "lint:fix": "eslint src/**/*.ts --config ../../.eslintrc --fix",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^9.4.0",
    "fast-glob": "^3.2.11",
    "sade": "^1.8.1",
    "tiny-glob": "^0.2.9"
  },
  "devDependencies": {
    "@types/sade": "^1.7.4"
  },
  "keywords": [
    "node_modules",
    "build",
    "environment",
    "dist",
    "installing",
    "package-management",
    "node-apps",
    "node-services",
    "services",
    "monorepos"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "yowainwright <yowainwright@gmail.com>",
  "homepage": "https://github.com/yowainwright/monorepo-utilities#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/monorepo-utilities.git"
  },
  "bugs": {
    "url": "https://github.com/yowainwright/monorepo-utilities/issues"
  },
  "jest": {
    "name": "@monorepo-utilities/install-dependencies",
    "projects": [
      "<rootDir>/packages/*"
    ],
    "resetMocks": true,
    "rootDir": "./",
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    }
  }
}
